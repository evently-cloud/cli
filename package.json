{
  "name": "@evently-cloud/cli",
  "description": "Evently.cloud CLI tools to develop applications and manage your ledgers.",
  "version": "0.2.0",
  "author": "Evently",
  "bin": {
    "evently": "./bin/run"
  },
  "bugs": "https://github.com/evently-cloud/cli/issues",
  "dependencies": {
    "@js-joda/core": "^5.5.3",
    "@node-rs/crc32": "^1.6.1",
    "@oclif/core": "2.8",
    "@oclif/plugin-commands": "2.2",
    "@oclif/plugin-help": "5.2",
    "@oclif/plugin-not-found": "2.3",
    "json5": "^2.2.3",
    "ketting": "^8.0.0-alpha.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.7",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "chai": "4.3.7",
    "env-cmd": "10.1.0",
    "eslint": "^8.36.0",
    "globby": "11.1.0",
    "mocha": "10.2.0",
    "oclif": "^3.8.1",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "^5.0.2"
  },
  "dirname": "evently",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/evently-cloud/cli",
  "keywords": [
    "evently",
    "cli",
    "tooling"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "oclif": {
    "bin": "evently",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": ":",
    "topics": {
      "ledger": {
        "description": "Ledger commands"
      },
      "registry": {
        "description": "Work with the event type registry"
      }
    },
    "update": {
      "s3": {
        "host": "https://s3.eu-central-003.backblazeb2.com",
        "bucket": "evently-cli"
      }
    }
  },
  "repository": "evently-cloud/cli",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "watch": "tsc --watch",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "pack": "oclif pack tarballs",
    "postpack": "rm -f oclif.manifest.json",
    "test": "mocha --forbid-only",
    "upload": "env-cmd -f .env/b2.env oclif upload tarballs",
    "lint": "eslint --quiet 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint --fix --quiet 'src/**/*.ts' 'test/**/*.ts'",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
