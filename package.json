{
  "name": "evently-cli",
  "description": "Evently.cloud CLI tools to develop applications and manage your ledgers.",
  "version": "0.0.1",
  "author": "Evently",
  "bin": {
    "evently": "./bin/run"
  },
  "bugs": "https://github.com/evently-cloud/cli/issues",
  "dependencies": {
    "@js-joda/core": "5.5.2",
    "@node-rs/crc32": "1.6.0",
    "@oclif/core": "2.1.7",
    "@oclif/plugin-commands": "2.2.10",
    "@oclif/plugin-help": "5.2.5",
    "@oclif/plugin-not-found": "2.3.20",
    "@oclif/plugin-update": "3.1.5",
    "ketting": "^7.5.1"
  },
  "devDependencies": {
    "@oclif/test": "2.3.7",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.14.0",
    "chai": "4.3.7",
    "env-cmd": "10.1.0",
    "globby": "11.1.0",
    "mocha": "10.2.0",
    "oclif": "3.6.5",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "dirname": "evently",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/evently-cloud/cli",
  "keywords": [
    "evently",
    "cli",
    "tooling"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "oclif": {
    "bin": "evently",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update"
    ],
    "topicSeparator": ":",
    "topics": {
      "ledger": {
        "description": "Ledger commands"
      }
    },
    "update": {
      "s3": {
        "host": "https://s3.eu-central-003.backblazeb2.com",
        "bucket": "evently-cli"
      }
    }
  },
  "repository": "evently-cloud/cli",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "watch": "tsc --watch",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "pack": "oclif pack tarballs",
    "postpack": "rm -f oclif.manifest.json",
    "test": "node --no-warnings node_modules/mocha/bin/_mocha --forbid-only \"test/**/*.test.ts\"",
    "upload": "env-cmd -f .env/b2.env oclif upload tarballs",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
